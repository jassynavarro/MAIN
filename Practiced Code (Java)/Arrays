public class PairedArrays {
    public static void main(String[] args) {
        
        int indexNum = 0;

        String email[] = {"scentpearl@gmail.com", "jassynavarro@gmail.com", "janenavarro@gmail.com"};
        String userName[] = {"scentPearl", "jassynavarro", "janenavarro"};
        String passWord[] = {"scentpearl@gmail.com", "jassynavarro@gmail.com", "janenavarro@gmail.com"};

        System.out.println("Index: " + indexNum);
        System.out.println("Email: " + email[indexNum]);
        System.out.println("UserName: " + userName[indexNum]);
        System.out.println("Password: " + passWord[indexNum]);
        
    }
}
